{
	"name": "the-builder",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome check .",
		"format": "biome format --write .",
		"preview": "vite preview",
		"postbuild": "node -e \"const fs=require('fs'); fs.copyFileSync('dist/index.html','dist/404.html');\"",
		"set:admin": "node scripts/set-admin.mjs",
